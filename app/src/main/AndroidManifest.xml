<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="top.navyblue.connerstone" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/myTheme" >
        <activity
            android:name=".views.activites.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/nLiveoDrawer" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.activites.BaseActivity"
            android:label="@string/title_activity_base" >
        </activity>
        <activity
            android:name=".views.activites.NavigationBaseActivity"
            android:label="@string/title_activity_navigation_base" >
        </activity>
    </application>

</manifest>
